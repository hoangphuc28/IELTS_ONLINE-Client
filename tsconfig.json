{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "baseUrl": ".",
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./*"],
      "@admin/*" :["./src/app/(admin)/*"],
      "@utils/*": ["./src/utils/*"],
      "@services/*": ["./src/services/*"],
      "@client/*": ["./src/app/(client)/*"],
      "@auth/*": ["./src/app/(auth)/*"],
      "@clientAccount/*": ["./src/app/(client)/account/*"],
      "@clientExamLibrary/*": ["./src/app/(client)/exam-library/*"],
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/app/(admin)/admin/parts/addedit/_components/editor/editor.tsxx", "src/app/(admin)/admin/parts/addedit/_components/editor/uploadAdapter.js", "src/app/(admin)/admin/parts/addedit/_components/editor/editor.tsx", "src/app/(admin)/util/timeFormat.js"],
  "exclude": ["node_modules"]
}
